@if (c.Resultado != null)
{
    @*<div>Resultado del Registro: @rs.Resultado</div>
        <div> Que significa: @rs.Cadena </div>
        <div>El resultado del mail es: @rs.CadenaMail</div>
        <div>A continuación, te dejo divs que se mostrarán según el resultado:</div>*@
    switch (c.Resultado)
    {
        case -4:
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <div class="alert alert-danger alert-dismissible fade show text-center" role="alert">
                        <strong>La el usuario y/o la contraseña introducidos no son correctos.</strong>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                </div>
            </div>

            break;
        case -3:
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <div class="alert alert-danger alert-dismissible fade show text-center" role="alert">
                        <strong>El teléfono introducido no es válido</strong>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                </div>
            </div>

            break;
        case -2:
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <div class="alert alert-danger alert-dismissible fade show text-center" role="alert">
                        <strong>La contraseña introducida no es válida</strong>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                </div>
            </div>
            break;
        case -1:
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6 text-center">
                    <div class="alert alert-danger alert-dismissible fade show text-center" role="alert">
                        <strong>El usuario ya existe</strong>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                </div>
            </div>


            break;
        case 0:
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6 ">
                    <div class="alert alert-info alert-dismissible fade show text-center" role="alert">
                        <strong> Se ha enviado la solicitud.  </strong> <br />Un administrador la revisará y recibirás noticias nuestras con la máxima brevedad posible.
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                </div>
            </div>
            break;
        case 1:
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6 ">
                    <div class="alert alert-info alert-dismissible fade show text-center" role="alert">
                        <div> Se ha procesado la solicitud. Revisa tu email para confirmar tu cuenta. </div>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                </div>
            </div>

            break;

    }

}

<div class="form-login d-none d-xl-block">
    <div class="col-md-12 col-xs-12">
        <div class="card">
            <div class="card-block p-2">
                <h4 class="text-center"> <i class="fas fa-lock"></i> Acceso Área Cliente</h4>
                <hr />
                <form asp-controller="AreaCliente" asp-action="Index" method="post">
                    <div class="form-group">
                        <label for="appointment_name" class="text-black">Usuario</label>
                        <input type="text" class="form-control" name="email" id="appointment_name">
                    </div>
                    <div class="form-group">
                        <label for="appointment_email" class="text-black">Contraseña</label>
                        <input type="text" class="form-control" name="password" id="appointment_email">
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-6">
                                <a href="#javascript," type="" class="" data-toggle="modal" data-target="#exampleModal">
                                    <small class="text-black">Recuperar Contraseña</small>
                                </a>
                            </div>
                            <div class="col-6 text-right"><input type="submit" value="Enviar" class="btn btn-primary"></div>
                        </div>



                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<section class="home-slider owl-carousel">
    @foreach (ImagenCabWeb cb in cab)
    {
        <div class="slider-item" style="background-image: url('@cb.url');">

            <div class="container">
                <div class="row slider-text align-items-center">
                    <div class="col-lg-7 col-md-7 col-sm-12 col-12 element-animate">
                        @if (cb.Subtitulo == "")
                        {
                            <h1>@cb.Titulo</h1>

                        }
                        else
                        {

                            <div class="row">
                                <div class="col-12 ">
                                    <h2 class="text-white" style="font-size:50px;">@cb.Titulo</h2>

                                </div>
                            </div>
                        }

                        <h2 class="text-center text-sm-center text-md-left text-lg-left text-white">@cb.Subtitulo</h2>
                        <p class="@cb.classcontent d-xl-block">@cb.Contenido</p>
                        <div class="d-xl-none text-center text-sm-center text-md-left text-lg-left">
                            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal1">
                                Acceso Área Clientes
                            </button>
                            <div>
                                <a href="#javascript," type="" class="" data-toggle="modal" data-target="#exampleModal" style="color:#fff;">
                                    <small class="">Recuperar Contraseña</small>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-5 col-xs-12"></div>

                </div>
            </div>
        </div>

    }
