@{ 
    @using dsASPCAtc.Web.ViewModels;
    @using EntidadesAtc;
    var vm = (ProductosViewModel)ViewData["Data"];
    var cm = (CampoBusqueda)ViewData["Data"];
}
<div class="page-bar">
    <ul class="page-breadcrumb">
        <li>
            <a asp-action="Home">Inicio</a>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <a asp-action="Home">Busqueda Genérica</a>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <span>@cm.cadena</span>
        </li>
    </ul>
</div>
<!-- BEGIN PAGE TITLE-->
<h1 class="page-title">
    Productos
    <small>@cm.cadena</small>
</h1>
<!-- END PAGE TITLE-->
<!-- INICIO TABLA -->
>
<div class="portlet light bordered" ng-controller="listaArticulos">
    <input type="text" value="@cm.cadena" ng-model="cadena" />
    <h1><small>Mostrando n artículos de m para </small>"@cm.cadena"</h1>
    <form action="Productos" method="post">
        @model CampoBusqueda;
        <input type="text" value="@vm.cm.FirstValor" name="FirstValor" />
        <input type="text" value="@vm.cm.LastValor" name="LastValor" />
        <input type="text" value="@vm.cm.FirstIndice" name="FirstIndice" />
        <input type="text" value="@vm.cm.LastIndice" name="LastIndice" />
        <input type="text" value="@vm.cm.cadena" name="cadena" />
        <input type="text" value="" name="AccionPagina" id="accionpagina" />
        <input type="submit" ng-model="first" value="Primera" onclick="cambiarPagina('F')" />
        <input type="submit" value="Anterior" onclick="cambiarPagina('P')" />
        <input type="submit" value="Siguiente" onclick="cambiarPagina('N')" />
        <input type="submit" ng-model="last" value="Última" onclick="cambiarPagina('L')" />
    </form>
    <table class="table table-striped table-bordered table-hover table-checkable order-column" id="sample_2">
        <thead>
            <tr>
                <th class="table-checkbox">
                </th>
                <th> Código </th>
                <th> Características </th>
                <th> Almacenes </th>
                <th>PVP</th>
                <th>%</th>
                <th>Neto</th>
                <th>Pedir</th>
            </tr>
        </thead>
        <tbody>
                <tr class="odd gradeX" ng-repeat="ar in articulos">
                    <td>
                        <img src="assets/layouts/layout/img/20180118124031.png" class="img-responsive" alt="">
                    </td>
                    <td>{{ar.codigo}}</td>
                    <td>
                        <a href=""> {{ar.descripcion}} </a>
                    </td>
                    <td>
                        <ul class="list-unstyled">
                            <li style="padding-bottom: 5px;">
                                Madrid  <span class="label label-sm label-success"> Disponible </span>
                            </li>
                            <li>
                                Tomelloso  <span class="label label-sm label-default"> Agotado </span>
                            </li>
                        </ul>

                    </td>
                    <td>0.00€</td>
                    <td>0%</td>
                    <td>0.00€</td>
                    <td>
                        <a href="javascript:;" class="btn btn-icon-only default">
                            <i class="fa fa-cart-plus"></i>
                        </a>
                    </td>
                </tr>
            <tr class="odd gradeX">
                <td>
                    <img src="assets/layouts/layout/img/default.png" class="img-responsive" alt="">
                </td>
                <td> 6548ACCVZ</td>
                <td>
                    <a href=""> PEUGEOT 207 PB REFLEC+SOPOR+VEN+PREMON ALREDEDOR</a>
                </td>
                <td>
                    <ul class="list-unstyled">
                        <li style="padding-bottom: 5px;">
                            Madrid  <span class="label label-sm label-warning"> Bajo Pedido </span>
                        </li>
                        <li>
                            Tomelloso  <span class="label label-sm label-warning"> Bajo Pedido </span>
                        </li>
                    </ul>

                </td>
                <td>0.00€</td>
                <td>0%</td>
                <td>0.00€</td>
                <td>
                    <a href="javascript:;" class="btn btn-icon-only default">
                        <i class="fa fa-cart-plus"></i>
                    </a>
                </td>
            </tr>
        </tbody>
    </table>
</div>
<!-- FIN TABLA  parabrisas-->
<!-- TABLA  Gomas-->
<div class="row">
    <div class="col-md-12">
        <div class="portlet light bordered">
            <h1>GRAPAS</h1>
        </div>
    </div>
</div>
